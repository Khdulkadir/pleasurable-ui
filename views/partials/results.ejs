  <div class="results">
    <% if (posts[0]) { %>
      <% posts.forEach(function(post) { %>
        <a href="/artikel/<%= post.slug %>">
          <article class="category-card">
            <p class="date"><%= post.date %></p>
      
            <h3><%- post.title.rendered %></h3>
            
            <% if (post.jetpack_featured_media_url[0]) { %>
              <img src="<%= post.yoast_head_json.og_image[0].url %>" alt="" width="<%= post.yoast_head_json.og_image[0].width %>" height="<%= post.yoast_head_json.og_image[0].height %>">
            <% } else { %>
              <img src="/assets/errorimg.png" alt="Kan foto niet laden">
            <% } %>
          </article>
        </a> 
      <% }) %>  
    <% } else {%>
      <h3> Er zijn geen artikelen</h3>
    <% } %>
  </div>